{
  "config": {
    "imports": {
      "react-provider": {
        "/": "*",
        "@arena/portal": "*",
        "@types/hoist-non-react-statics": "*",
        "@types/prop-types": "*",
        "@types/react": "*",
        "@types/react-redux": "*",
        "@arena/lib-component-common": "*",
        "@arena/lib-portlet-common": "*",
        "@babel/runtime": "*",
        "asynckit": "*",
        "axios": "*",
        "combined-stream": "*",
        "csstype": "*",
        "delayed-stream": "*",
        "dom-helpers": "*",
        "event-source-polyfill": "*",
        "fast-json-stable-stringify": "*",
        "follow-redirects": "*",
        "form-data": "*",
        "history": "*",
        "hoist-non-react-statics": "*",
        "html-escaper": "*",
        "html-parse-stringify": "*",
        "i18next": "*",
        "i18next-chained-backend": "*",
        "i18next-localstorage-backend": "*",
        "i18next-xhr-backend": "*",
        "isarray": "*",
        "js-tokens": "*",
        "loose-envify": "*",
        "mime-db": "*",
        "mime-types": "*",
        "mini-create-react-context": "*",
        "object-assign": "*",
        "path-to-regexp": "*",
        "property-expr": "*",
        "react-fast-compare": "*",
        "react-i18next": "*",
        "react-is": "*",
        "react-redux": "*",
        "react-router": "*",
        "react-router-dom": "*",
        "redux": "*",
        "redux-persist": "*",
        "redux-thunk": "*",
        "regenerator-runtime": "*",
        "resolve-pathname": "*",
        "rw": "*",
        "safer-buffer": "*",
        "tiny-case": "*",
        "tiny-invariant": "*",
        "tiny-warning": "*",
        "tinycolor2": "*",
        "toposort": "*",
        "tslib": "*",
        "type-fest": "*",
        "value-equal": "*",
        "void-elements": "*",
        "warning": "*",
        "yup": "*"
      }
    }
  },
  "rules": [
    {
      "test": "\\.json$",
      "use": [
        "json-loader"
      ]
    },
    {
      "test": "\\.css$",
      "use": [
        "css-loader"
      ]
    },
    {
      "test": "^assets/",
      "exclude": [
        "node_modules",
        "\\.scss$"
      ],
      "use": [
        "copy-loader"
      ]
    }
  ],
  "sources": [
    "src",
    "assets"
  ],
  "create-jar": {
    "features": {
      "js-extender": true,
      "localization": "features/localization/Language",
      "configuration": "features/configuration.json"
    }
  },
  "dump-report": true,
  "max-parallel-files": 128,
  "verbose": true
}
