{
  "name": "react-remote-portlet",
  "version": "1.0.0",
  "description": "React Remote Portlet",
  "browserslist": {
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ],
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ]
  },
  "dependencies": {
    "@liferay/portal-7.4": "*"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "cross-env": "~7.0.3",
    "@babel/cli": "~7.21.0",
    "@babel/core": "~7.21.0",
    "@babel/plugin-proposal-class-properties": "~7.18.6",
    "@babel/plugin-proposal-nullish-coalescing-operator": "~7.18.6",
    "@babel/plugin-syntax-flow": "~7.21.4",
    "@babel/plugin-transform-react-jsx": "~7.21.0",
    "@babel/plugin-transform-runtime": "~7.21.4",
    "@babel/preset-env": "^7.21.4",
    "@babel/preset-react": "^7.18.6",
    "@babel/runtime": "^7.21.0"
  },
  "scripts": {
    "babel": "cross-env NODE_ENV=production babel --source-maps -d src src/*.jsx",
    "build": "run-s babel && liferay build",
    "clean": "liferay clean",
    "test": "true",
    "deploy": "liferay deploy",
    "start": "liferay start"
  }
}
