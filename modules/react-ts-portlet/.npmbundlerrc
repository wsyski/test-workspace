{
  "config": {
    "imports": {
      "react-provider": {
        "/": "*",
        "@arena/lib-portlet-common": "*",
        "@babel/runtime": "*",
        "axios": "*",
        "i18next": "*",
        "i18next-chained-backend": "*",
        "i18next-localstorage-backend": "*",
        "i18next-xhr-backend": "*",
        "react-i18next": "*",
        "react-router": "*",
        "react-router-dom": "*"
      }
    },
    "strictGlobalDependencies": true
  },
  "exclude": {
    "*": ["**/__mocks__/**/*", "**/__tests__/**/*"],
    "react-provider": true,
    "@arena/lib-portlet-common": true,
    "@babel/runtime": true,
    "axios": true,
    "i18next": true,
    "i18next-chained-backend": true,
    "i18next-localstorage-backend": true,
    "i18next-xhr-backend": true,
    "react-i18next": true,
    "react-router": true,
    "react-router-dom": true
  },
  "create-jar": {
    "features": {
      "js-extender": true,
      "web-context": "/react-ts-portlet",
      "localization": "features/localization/Language",
      "configuration": "features/configuration.json"
    }
  },
  "dump-report": true,
  "max-parallel-files": 128,
  "verbose": true
}
