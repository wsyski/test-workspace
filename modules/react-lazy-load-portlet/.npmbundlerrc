{
  "config": {
    "imports": {
      "react-provider": {
        "/": "*",
        "@arena/lib-portlet-common": "*",
        "@babel/runtime": "*"
      }
    },
    "strictGlobalDependencies": true
  },
  "exclude": {
    "*": ["**/__mocks__/**/*", "**/__tests__/**/*"],
    "@arena/lib-portlet-common": true,
    "@babel/runtime": true,
    "react-provider": true
  },
  "create-jar": {
    "features": {
      "js-extender": true,
      "web-context": "/react-lazy-load-portlet",
      "localization": "features/localization/Language",
      "configuration": "features/configuration.json"
    }
  },
  "dump-report": true,
  "max-parallel-files": 128,
  "verbose": true
}
