{
  "presets": [
    ["@babel/preset-env", { "targets": {"node": "current"} }],
    [
      "@babel/preset-react"
    ]
  ],
  "plugins": [
    "@babel/plugin-proposal-optional-chaining",
    "@babel/plugin-transform-class-properties",
    "@babel/plugin-transform-runtime"
  ],
  "ignore": [
    "jest.config.js",
    "src/setupTests.js",
    "**/__mocks__/*",
    "**/__tests__/*"
  ]
}
