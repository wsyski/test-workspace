{
  "name": "react-lazy-load-portlet",
  "version": "1.0.0",
  "description": "React lazy load portlet",
  "private": true,
  "license": "UNLICENSED",
  "main": "index.js",
  "dependencies": {
    "@arena/lib-portlet-common": "*",
    "@liferay/portal-7.4": "*",
    "@arena/portal": "*",
    "react-feather": "^1.1.6",
    "styled-components": "^5.3.6"
  },
  "devDependencies": {
    "@types/enzyme": "~3.10.12",
    "@types/enzyme-adapter-react-16": "~1.0.6",
    "@types/react-router-dom": "5.3.3",
    "@types/react-test-renderer": "~16.9.3",
    "@types/testing-library__react-hooks": "~3.4.1"
  },
  "resolutions": {
  },
  "portlet": {
    "com.liferay.portlet.display-category": "category.sample",
    "com.liferay.portlet.header-portlet-css": "/css/styles.css",
    "com.liferay.portlet.instanceable": true,
    "javax.portlet.name": "react_lazy_load_portlet",
    "javax.portlet.security-role-ref": "power-user,user",
    "javax.portlet.resource-bundle": "content.Language"
  },
  "scripts": {
    "sass": "sass assets/css/styles.scss assets/css/styles.css --source-map",
    "babel": "cross-env NODE_ENV=production babel --source-maps -d build src",
    "build": "run-s sass && run-s babel && cross-env NODE_ENV=production liferay-npm-bundler",
    "clean": "liferay clean",
    "deploy": "gulp deploy",
    "publish": "gulp publish",
    "start": "react-scripts start",
    "test": "true"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
