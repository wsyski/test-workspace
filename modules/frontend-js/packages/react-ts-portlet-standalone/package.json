{
  "name": "react-ts-portlet-standalone",
  "version": "1.0.0",
  "description": "React Portlet Typescript Standalone",
  "license": "UNLICENSED",
  "main": "index.js",
  "dependencies": {
    "i18next": "~21.6.10",
    "i18next-chained-backend": "~3.0.2",
    "i18next-localstorage-backend": "~3.1.3",
    "i18next-xhr-backend": "3.2.2",
    "react-i18next": "~11.18.6",
    "@liferay/portal-7.4": "*"
  },
  "devDependencies": {
    "@babel/cli": "~7.20.7",
    "@babel/core": "~7.20.12",
    "@babel/plugin-proposal-nullish-coalescing-operator": "~7.18.6",
    "@babel/plugin-transform-runtime": "~7.24.7",
    "@babel/preset-env": "~7.20.2",
    "@babel/preset-react": "~7.18.6",
    "@babel/preset-typescript": "~7.23.3",
    "@types/enzyme": "~3.1.18",
    "@types/enzyme-adapter-react-16": "~1.0.9",
    "@types/jest": "~26.0.24",
    "@types/react": "^16.0.0",
    "@types/react-dom": "^16.0.0",
    "@types/warning": "~3.0.0",
    "cross-env": "^7.0.3",
    "enzyme": "~3.11.0",
    "enzyme-adapter-react-16": "~1.15.6",
    "enzyme-to-json": "~3.6.1",
    "jest": "~26.6.3",
    "jest-environment-jsdom": "~26.6.2",
    "npm-run-all": "~4.1.5",
    "rimraf": "~5.0.0",
    "ts-jest": "~26.5.6",
    "typescript": "4.4.2"
  },
  "resolutions": {
    "@types/react": "^16.0.0",
    "@types/react-dom": "^16.0.0",
    "react": "16.12.0",
    "react-dom": "16.12.0"
  },
  "portlet": {
    "com.liferay.portlet.display-category": "category.sample",
    "com.liferay.portlet.header-portlet-css": "/css/styles.css",
    "com.liferay.portlet.instanceable": true,
    "javax.portlet.name": "react_ts_portlet_standalone",
    "javax.portlet.security-role-ref": "power-user,user",
    "javax.portlet.resource-bundle": "content.Language"
  },
  "scripts": {
    "sass": "sass assets/css/styles.scss assets/css/styles.css --source-map",
    "build": "run-p tsc sass && run-s babel && cross-env NODE_ENV=production liferay-npm-bundler",
    "clean": "rimraf out-tsc && liferay clean",
    "deploy": "gulp deploy",
    "tsc": "tsc -b tsconfig.prod.json",
    "babel": "cross-env NODE_ENV=production babel --source-maps -d build out-tsc",
    "test": "jest --verbose",
    "start": "react-scripts start"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
