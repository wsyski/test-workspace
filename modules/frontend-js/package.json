{
  "name": "frontend-js",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": ">=20",
    "yarn": "1.x"
  },
  "devDependencies": {
    "@babel/cli": "~7.20.7",
    "@babel/core": "~7.20.12",
    "@babel/plugin-proposal-nullish-coalescing-operator": "~7.18.6",
    "@babel/preset-env": "~7.20.2",
    "@babel/preset-react": "~7.18.6",
    "@babel/preset-typescript": "~7.23.3",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^24.1.0",
    "@rollup/plugin-node-resolve": "^15.0.2",
    "@rollup/plugin-terser": "^0.4.1",
    "@testing-library/react": "~12.1.5",
    "@testing-library/user-event": "~12.8.3",
    "@types/jest": "~26.0.24",
    "@types/lodash": "^4.14.119",
    "@types/node": "^16.11.27",
    "@types/react": "^16.0.0",
    "@types/react-dom": "^16.0.0",
    "@types/react-redux": "7.1.25",
    "@types/react-router": "5.1.18",
    "@types/warning": "~3.0.0",
    "@types/yup": "^0.24.9",
    "cpr": "^3.0.1",
    "cpx": "^1.5.0",
    "cross-env": "^7.0.3",
    "enzyme": "~3.11.0",
    "enzyme-adapter-react-16": "~1.15.6",
    "enzyme-to-json": "~3.6.1",
    "gulp": "^4.0.2",
    "jest": "~26.6.3",
    "jest-environment-jsdom": "~26.6.2",
    "npm-run-all": "^4.1.5",
    "nx": "~18.3.4",
    "react-test-renderer": "16.12.0",
    "rimraf": "^5.0.0",
    "rollup": "^3.18.0",
    "ts-jest": "~26.5.6",
    "typescript": "4.4.2"
  },
  "resolutions": {
    "@types/react": "^16.0.0",
    "@types/react-dom": "^16.0.0",
    "@types/react-router-dom": "5.3.3",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-test-renderer": "16.12.0",
    "ansis": "1.5.1",
    "axios": "0.27.2"
  },
  "scripts": {
    "workspace-install": "yarn install",
    "build:all": "nx run-many -t build",
    "build": "run-s reset:nx && run-s build:all",
    "build:libs": "nx run-many -t build -p @arena/lib-liferay-mock @arena/portal @arena/lib-portlet-common",
    "clean": "nx run-many -t clean",
    "deploy": "nx run-many -t deploy",
    "format": "true",
    "publish": "yarn workspaces run publish",
    "reset:nx": "nx reset",
    "test": "nx run-many -t test --verbose"
  },
  "workspaces": {
    "packages": [
      "packages/portal",
      "packages/libs/packages/*",
      "packages/*"
    ]
  }
}
