{
  "name": "frontend-js",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": ">=20",
    "yarn": "1.x"
  },
  "devDependencies": {
    "@babel/cli": "~7.25.7",
    "@babel/plugin-transform-runtime": "~7.25.7",
    "@babel/plugin-proposal-optional-chaining": "~7.21.0",
    "@babel/plugin-proposal-class-properties": "~7.18.6",
    "@babel/plugin-proposal-nullish-coalescing-operator": "~7.18.6",
    "@babel/plugin-transform-react-jsx": "~7.25.7",
    "@liferay/eslint-plugin": "~1.5.0",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^28.0.0",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@testing-library/react": "~12.1.5",
    "@testing-library/user-event": "~12.8.3",
    "@types/enzyme": "~3.10.18",
    "@types/enzyme-adapter-react-16": "~1.0.9",
    "@types/jest": "~28.1.8",
    "@types/lodash": "4.17.5",
    "@types/node": "~20.15.0",
    "@types/react": "^16.14.1",
    "@types/react-dom": "^16.9.7",
    "@types/react-redux": "7.1.25",
    "@types/react-router": "5.1.18",
    "@types/warning": "~3.0.0",
    "@types/yup": "^0.24.9",
    "babel-eslint": "10.0.2",
    "cpx": "^1.5.0",
    "cross-env": "^7.0.3",
    "enzyme": "~3.11.0",
    "enzyme-adapter-react-16": "~1.15.8",
    "enzyme-to-json": "~3.6.2",
    "gulp": "^4.0.2",
    "jest": "~28.1.3",
    "jest-environment-jsdom": "~28.1.3",
    "npm-run-all": "^4.1.5",
    "nx": "~20.0.0",
    "react-test-renderer": "16.12.0",
    "rimraf": "^6.0.1",
    "rollup": "^4.24.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "sass": "^1.79.4",
    "ts-jest": "~28.0.8",
    "typescript": "4.4.2"
  },
  "resolutions": {
    "@types/react": "^16.14.1",
    "@types/react-dom": "^16.9.7",
    "@types/react-router-dom": "5.3.3",
    "cheerio": "1.0.0-rc.3",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-test-renderer": "16.12.0",
    "ansis": "1.5.1",
    "axios": "0.27.2"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "es6": true,
      "jest": true,
      "node": true
    },

    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "ecmaVersion": 2020
    },
    "overrides": [
      {
        "env": {
          "jest": true
        },
        "files": [
          "**/__tests__/**/*.js",
          ",**/test/**/*.js",
          "**/tests/**/*.js"
        ]
      }
    ],
    "plugins": ["@liferay"],
    "extends": ["plugin:@liferay/react"],
    "globals": {
      "Liferay": true
    },
    "rules": {
      "@liferay/no-anonymous-exports": "off",
      "@typescript-eslint/member-ordering": "off",
      "react/jsx-fragments": "off"
    }
  },
  "scripts": {
    "workspace-install": "yarn install",
    "build:all": "nx run-many -t build",
    "build:libs": "nx run-many -t build -p @arena/lib-liferay-mock @arena/portal @arena/lib-portlet-common",
    "build": "run-s reset:nx && run-s build:all",
    "clean": "nx run-many -t clean",
    "deploy": "nx run-many -t deploy",
    "format": "eslint 'packages/**/{src,test/src}/**/*.{js,ts,tsx}' --fix",
    "publish": "yarn workspaces run publish",
    "reset:nx": "nx reset",
    "test": "nx run-many -t test --verbose"
  },
  "workspaces": {
    "packages": [
      "packages/portal",
      "packages/libs/packages/*",
      "packages/*"
    ]
  }
}
