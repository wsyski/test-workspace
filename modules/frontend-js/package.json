{
  "name": "frontend-js",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": "=16",
    "yarn": "1.x"
  },
  "devDependencies": {
    "@liferay/eslint-plugin": "~1.4.0",
    "@babel/cli": "~7.20.7",
    "@babel/core": "~7.20.12",
    "@babel/plugin-proposal-nullish-coalescing-operator": "~7.18.6",
    "@babel/preset-env": "~7.20.2",
    "@babel/preset-react": "~7.18.6",
    "@babel/plugin-transform-runtime": "~7.18.10",
    "babel-eslint": "~10.1.0",
    "typescript": "~4.4.4"
  },
  "dependencies": {
  },
  "resolutions": {
    "@types/react": "16.9.56",
    "@types/react-dom": "16.9.14",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-test-renderer": "16.12.0",
    "ansis": "1.5.1"
  },
  "scripts": {
    "workspace-install": "yarn install",
    "build": "yarn run format && yarn workspaces run build",
    "build:lib": "yarn workspace @arena/lib-portlet-common build",
    "clean": "yarn workspaces run clean",
    "deploy": "yarn workspaces run deploy",
    "format": "eslint packages/**/{src,test/src}/**/*.{ts,tsx} --fix",
    "publish": "yarn workspaces run publish",
    "test": "yarn workspaces run test"
  },
  "workspaces": {
    "packages": [
      "packages/portal",
      "packages/lib-liferay-mock",
      "packages/lib-portlet-common",
      "packages/*"
    ]
  }
}
