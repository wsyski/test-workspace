  "eslintConfig": {
    "env": {
      "browser": true,
      "jest": true,
      "node": true
    },
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "ecmaVersion": 2015
    },
    "overrides": [
      {
        "env": {
          "jest": true
        },
        "files": [
          "**/__tests__/**/*.js",
          ",**/test/**/*.js",
          "**/tests/**/*.js"
        ]
      }
    ],
    "plugins": [
      "@liferay"
    ],
    "extends": [
      "plugin:@liferay/react"
    ],
    "globals": {
      "Liferay": true
    },
    "rules": {
      "@liferay/no-anonymous-exports": "off",
      "@typescript-eslint/member-ordering": "off",
      "react/jsx-fragments": "off"
    }
  },
  "scripts": {
    "workspace-install": "yarn install",
    "build:all": "nx run-many -t build",
    "build:libs": "nx run-many -t build -p @arena/lib-liferay-mock @arena/lib-portlet-test @arena/portal @arena/lib-component-common @arena/lib-portlet-common",
    "build": "run-s reset:nx && run-s build:all",
    "clean": "nx run-many -t clean",
    "deploy": "nx run-many -t deploy",
    "format": "eslint 'packages/**/{src,test/src}/**/*.{js,ts,tsx}' --fix",
    "publish": "yarn workspaces run publish",
    "reset:nx": "nx reset",
    "test": "nx run-many -t test --verbose"
  },
  "workspaces": {
    "packages": [
      "packages/portal",
      "packages/libs/packages/*",
      "packages/*"
    ]
  }
}
